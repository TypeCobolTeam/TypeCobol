           END-IF.                                                      CMM010AK
      * Comptabilisation du 1er NOVEMBRE                                CMM010AK
           IF WS-CMM010-MMJJX >= '1101'                                 CMM010AK
             THEN                                                       CMM010AK
      *        si le 1er Novembre n'a pas ete compt. (samedi ou dimancheCMM010AK
               IF DTIN-PARA017 = '2' AND WS-CMM010-1ERJ-BISS NOT = 3    CMM010AK
                                     AND WS-CMM010-1ERJ-BISS NOT = 4    CMM010AK
                 THEN PERFORM A9791-CMM010-CALCUL-JF2                   CMM010AK
               END-IF                                                   CMM010AK
      *        si le 1er Novembre n'a pas ete comptabilise (dimanche)   CMM010AK
               IF DTIN-PARA017 = '1' AND WS-CMM010-1ERJ-BISS NOT = 4    CMM010AK
                 THEN PERFORM A9792-CMM010-CALCUL-JF1                   CMM010AK
               END-IF                                                   CMM010AK
           END-IF.                                                      CMM010AK
      * Comptabilisation du 11 NOVEMBRE                                 CMM010AK
           IF WS-CMM010-MMJJX >= '1111'                                 CMM010AK
             THEN                                                       CMM010AK
      *        si le 11 Novembre n'a pas ete compt. (samedi ou dimanche)CMM010AK
               IF DTIN-PARA017 = '2' AND WS-CMM010-1ERJ-BISS NOT = 7    CMM010AK
                                     AND WS-CMM010-1ERJ-BISS NOT = 8    CMM010AK
                                     AND WS-CMM010-1ERJ-BISS NOT = 1    CMM010AK
                 THEN PERFORM A9791-CMM010-CALCUL-JF2                   CMM010AK
               END-IF                                                   CMM010AK
      *        si le 11 Novembre n'a pas ete comptabilise (dimanche)    CMM010AK
               IF DTIN-PARA017 = '1' AND WS-CMM010-1ERJ-BISS NOT = 1    CMM010AK
                                     AND WS-CMM010-1ERJ-BISS NOT = 8    CMM010AK
                 THEN PERFORM A9792-CMM010-CALCUL-JF1                   CMM010AK
               END-IF                                                   CMM010AK
           END-IF.                                                      CMM010AK
      * Comptabilisation du 25 DECEMBRE                                 CMM010AK
           IF WS-CMM010-MMJJX >= '1225'                                 CMM010AK
             THEN                                                       CMM010AK
