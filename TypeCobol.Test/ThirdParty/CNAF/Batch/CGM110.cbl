               IF DTIN-PARA017 = '2' AND WS-CMM010-1ERJ-BISS NOT = 1    CMM010AK
                                     AND WS-CMM010-1ERJ-BISS NOT = 8    CMM010AK
                                     AND WS-CMM010-1ERJ-BISS NOT = 2    CMM010AK
