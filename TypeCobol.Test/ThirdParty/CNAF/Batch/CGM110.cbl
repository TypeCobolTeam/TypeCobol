       CGBCALAK-CVC-V00930.                                             CGBCALAK
      *----------------------------------------------------             CGBCALAK
      * SPECIAL CFVC V00930                               -             CGBCALAK
      *----------------------------------------------------             CGBCALAK
           MOVE 1 TO WS-ZRTGEN.                                         CGBCALAK
           IF WS-ZENTLG = 'DRC'                                         CGBCALAK
           OR WS-ZENTLG = 'DRV'                                         CGBCALAK
            MOVE 0 TO WS-ZRTGEN                                         CGBCALAK
            IF WS-ZFONCT (1:1) = 'C'                                    CGBCALAK
            OR WS-ZFONCT (1:1) = 'M'                                    CGBCALAK
            OR WS-ZFONCT (1:1) = 'S'                                    CGBCALAK
             IF WS-CLE-INF    (05:3) NOT = 'AES'                        CGBCALAK
              MOVE 1 TO WS-ZRTGEN                                       CGBCALAK
             END-IF                                                     CGBCALAK
            END-IF                                                      CGBCALAK
           ELSE                                                         CGBCALAK
            IF WS-ZFONCT (1:1) = 'C'                                    CGBCALAK
            OR WS-ZFONCT (1:1) = 'M'                                    CGBCALAK
            OR WS-ZFONCT (1:1) = 'S'                                    CGBCALAK
             MOVE 1 TO WS-ZRTGEN                                        CGBCALAK
             MOVE 1 TO WS-ZRTGEN                                        CGBCALAK
            ELSE                                                        CGBCALAK
             MOVE 0 TO WS-ZRTGEN                                        CGBCALAK
            END-IF                                                      CGBCALAK
           END-IF.                                                      CGBCALAK
           IF WS-ZRTGEN = 1                                             CGBCALAK
            SET FL-ZSOLDE-SOLDEE  TO TRUE                               CGBCALAK
            MOVE 0 TO WS-ZNOCCD                                         CGBCALAK
            MOVE 0 TO WS-ZRTDET                                         CGBCALAK
      D     DISPLAY '(CGBCALAK-CVC-V00930) '                            CGBCALAK
      D             WS-ZENTLG '-' WS-ZIDGDD ' ' WS-ZFONCT               CGBCALAK
