       XXXH-CAL-TH0-1-2-3.                                              CGBCALAK
      *--------------------------------------------------------         CGBCALAK
      * CONTROLE EXISTENCE DU GDD SUR TABLEAUX MAJEURS TH0 TH1 TH2 TH3  CGBCALAK
      * DONNEE EN ENTREE WSBA-ZTABLE2 NOM DE LA TABLE (par ex. DOH)     CGBCALAK
      *                  WSBA-ZIDGDD2 NOM DU GDD DANS LA TABLE          CGBCALAK
      *                  (par ex. MSD)                                  CGBCALAK
      * DONNEE EN SORTIE IX-TH0 IX-TH1 IX-TH2 IX-TH3                    CGBCALAK
      *-----------*                                                     CGBCALAK
      * xx- On a trouv le gdd WSBA-ZIDGDD2 dans TH2 liste des tabgdd   CGBCALAK
      *--------------------------------------------------------         CGBCALAK
      *-CIx-                                                            CGBCALAK
      *-                                                                CGBCALAK
           MOVE 0 TO WSBA-ZRESUL                                        CGBCALAK
      D    if ws-displ = 'O'                                            CGBCALAK
      D       display '(CGBCALAK)-CI1- WSBA-ZTABLE2 ZIDGDD2: '          CGBCALAK
      D       wsba-ztable2   wsba-zidgdd2                               CGBCALAK
      D    end-if                                                       CGBCALAK
           PERFORM VARYING IX-TH0 FROM 1 BY 1                           CGBCALAK
           UNTIL IX-TH0 = IX-TH0-M                                      CGBCALAK
           OR TH0-TAB (IX-TH0) = WSBA-ZTABLE2                           CGBCALAK
             CONTINUE                                                   CGBCALAK
           END-PERFORM.                                                 CGBCALAK
                                                                        CGBCALAK
           IF IX-TH0 < IX-TH0-M                                         CGBCALAK
              SET IX-TH1 TO IX-TH0                                      CGBCALAK
                SET IX-TH2 TO TH1-ORI (IX-TH1)                          CGBCALAK
                IF WSBA-ZIDGDD2 = SPACE                                 CGBCALAK
                  MOVE TH2-GDD (IX-TH2) TO WSBA-ZIDGDD2                 CGBCALAK
                END-IF                                                  CGBCALAK
      *                                                                 CGBCALAK
              PERFORM VARYING IX-TH2 FROM IX-TH2 BY 1                   CGBCALAK
              UNTIL IX-TH2 = TH1-ORI (IX-TH1 + 1)                       CGBCALAK
              OR TH2-GDD (IX-TH2) = WSBA-ZIDGDD2                        CGBCALAK
                 CONTINUE                                               CGBCALAK
              END-PERFORM                                               CGBCALAK
              IF IX-TH2 < TH1-ORI (IX-TH1 + 1)                          CGBCALAK
      *---       xx- On a trouv le gdd WSBA-ZIDGDD2 dans TH2           CGBCALAK
                 SET IX-TH3 TO IX-TH2                                   CGBCALAK
              ELSE                                                      CGBCALAK
      D          display '(CGBCALAK)-CI2- ERREUR '                      CGBCALAK
      D             wsba-zidgdd2 ' inconnu de CGLBAMAJ '                CGBCALAK
                 MOVE 1 TO WSBA-ZRESUL                                  CGBCALAK
              END-IF                                                    CGBCALAK
           ELSE                                                         CGBCALAK
      D       display '(CGBCALAK)-CI3- ERREUR Identifiant de table '    CGBCALAK
      D          wsba-ztable2 ' inconnu de CGLBAMAJ '                   CGBCALAK
              MOVE 1 TO WSBA-ZRESUL                                     CGBCALAK
           END-IF.                                                      CGBCALAK
                                                                        CGBCALAK
       XXXH-CAL-TH0-1-2-3-DICHOT.                                       CGBCALAK
      *-------------------------------------------------                CGBCALAK
      *  recherche dichotomique sur th0 th1 th2 th3                     CGBCALAK
      *  de la table db2 recherch et du gdd recherch                  CGBCALAK
      *-------------------------------------------------                CGBCALAK
      *-CJx-                                                            CGBCALAK
      *-                                                                CGBCALAK
           PERFORM VARYING IX-TH0 FROM 1 BY 1                           CGBCALAK
           UNTIL IX-TH0 = IX-TH0-M                                      CGBCALAK
           OR TH0-TAB (IX-TH0) = WSBA-ZTABLE                            CGBCALAK
             CONTINUE                                                   CGBCALAK
           END-PERFORM.                                                 CGBCALAK
                                                                        CGBCALAK
           IF IX-TH0 < IX-TH0-M                                         CGBCALAK
             SET IX-TH1 TO IX-TH0                                       CGBCALAK
      *d     display 'CGBCALAK -1- -CAL-TH0-1-2-3-DICHOT wsba-zresul=1' CGBCALAK
             MOVE 1     TO WSBA-ZRESUL                                  CGBCALAK
           ELSE                                                         CGBCALAK
      D      display 'CGBCALAK -2- -CAL-TH0-1-2-3-DICHOT wsba-zresul=9' CGBCALAK
             MOVE 9 TO WSBA-ZRESUL                                      CGBCALAK
           END-IF.                                                      CGBCALAK
                                                                        CGBCALAK
           IF WSBA-ZRESUL = 1                                           CGBCALAK
      *d     display 'CGBCALAK -3- -CAL-TH0-1-2-3-DICHOT wsba-zresul'   CGBCALAK
      *d        '=1 remis  0'                                          CGBCALAK
             MOVE ZERO TO WSBA-ZRESUL                                   CGBCALAK
             MOVE TH1-ORI (IX-TH1) TO WSBA-MINI                         CGBCALAK
             MOVE TH1-ORI (IX-TH1 + 1) TO WSBA-MAXI                     CGBCALAK
             COMPUTE WSBA-MAXI = WSBA-MAXI - 1                          CGBCALAK
             PERFORM UNTIL WSBA-MAXI = 0 OR                             CGBCALAK
                    (TH2-GDD (WSBA-MAXI) < '000' OR                     CGBCALAK
                     TH2-GDD (WSBA-MAXI) > '999'     )                  CGBCALAK
               SUBTRACT 1 FROM WSBA-MAXI                                CGBCALAK
             END-PERFORM                                                CGBCALAK
             PERFORM UNTIL WSBA-ZRESUL > ZERO                           CGBCALAK
               COMPUTE LSBA-CENT = (WSBA-MINI + WSBA-MAXI) / 2          CGBCALAK
               IF WSBA-MINI > WSBA-MAXI                                 CGBCALAK
      D           display 'CGBCALAK -4- -CAL-TH0-1-2-3-DICHOT '         CGBCALAK
      D              ' WSBA-MINI > WSBA-MAXI -> wsba-zresul = 9 '       CGBCALAK
                 MOVE 9 TO WSBA-ZRESUL                                  CGBCALAK
               ELSE                                                     CGBCALAK
                 IF TH2-GDD (LSBA-CENT) = LSBA-ZIDGDD                   CGBCALAK
                   SET IX-TH2 TO LSBA-CENT                              CGBCALAK
                   SET IX-TH3 TO LSBA-CENT                              CGBCALAK
                   MOVE 1 TO WSBA-ZRESUL                                CGBCALAK
      *d          display 'CGBCALAK -5- -CAL-TH0-1-2-3-DICHOT '         CGBCALAK
      *d          display '         -5- th2-gdd (lsba-cent) : '         CGBCALAK
      *d             th2-gdd (lsba-cent) ' = ' lsba-zidgdd              CGBCALAK
      *d             ' -> wsba-zresul = 1'                              CGBCALAK
                 ELSE                                                   CGBCALAK
                   IF TH2-GDD (LSBA-CENT) < LSBA-ZIDGDD                 CGBCALAK
                     COMPUTE WSBA-MINI = LSBA-CENT + 1                  CGBCALAK
                   ELSE                                                 CGBCALAK
                     COMPUTE WSBA-MAXI = LSBA-CENT - 1                  CGBCALAK
                   END-IF                                               CGBCALAK
                 END-IF                                                 CGBCALAK
               END-IF                                                   CGBCALAK
             END-PERFORM                                                CGBCALAK
           END-IF.                                                      CGBCALAK
                                                                        CGBCALAK
           IF WSBA-ZRESUL = 1                                           CGBCALAK
      *d      display 'CGBCALAK -6- wsba-zresul = 1 remis  0 '         CGBCALAK
             MOVE ZERO TO WSBA-ZRESUL                                   CGBCALAK
           END-IF.                                                      CGBCALAK
      D    IF WSBA-ZRESUL not = zero                                    CGBCALAK
      D      display '(BCAL) -TH0-1-2-3-DICHOT WSBA-ZRESUL : '          CGBCALAK
      D       WSBA-ZRESUL                                               CGBCALAK
      D    end-if                                                       CGBCALAK
      D    IF WSBA-ZRESUL = 9                                           CGBCALAK
      D       DISPLAY '(BCAL) INCOHERENCE TABLE = ' WSBA-ZTABLE         CGBCALAK
      D        ' ET GDD  = ' LSBA-ZIDGDD                                CGBCALAK
      D    END-IF.                                                      CGBCALAK
