      ******************************************************************CMM010AK
      * Cadrage sur un jour de la semaine (Options 22 et 32)           *CMM010AK
      * entree : WS-CMM010-CADR-BIN code cadrage en binaire            *CMM010AK
      * entree et sortie : WS-CMM010-ANNE-BIN, WS-CMM010-MOIS-BIN      *CMM010AK
      *                    WS-CMM010-JOUR-BIN, WS-CMM010-NUM-SEM       *CMM010AK
      ******************************************************************CMM010AK
                                                                        CMM010AK
           IF FL-CMM010-BISS                                            CMM010AK
             THEN MOVE 29 TO WS-CMM010-NBJJ (2)                         CMM010AK
           END-IF                                                       CMM010AK
           COMPUTE WS-CMM010-JOUR-BIN = WS-CMM010-JOUR-BIN              CMM010AK
                                      + WS-CMM010-CADR-BIN              CMM010AK
                                      - WS-CMM010-NUM-SEM               CMM010AK
           IF WS-CMM010-NUM-SEM < WS-CMM010-CADR-BIN                    CMM010AK
             THEN                                                       CMM010AK
               IF WS-CMM010-JOUR-BIN >                                  CMM010AK
                                 WS-CMM010-NBJJ (WS-CMM010-MOIS-BIN)    CMM010AK
                 THEN                                                   CMM010AK
                   IF WS-CMM010-MOIS-BIN = 12                           CMM010AK
                     THEN                                               CMM010AK
                       ADD 1 TO WS-CMM010-ANNE-BIN                      CMM010AK
                       MOVE 1 TO WS-CMM010-MOIS-BIN                     CMM010AK
                       SUBTRACT 31 FROM WS-CMM010-JOUR-BIN              CMM010AK
                     ELSE                                               CMM010AK
                       SUBTRACT WS-CMM010-NBJJ (WS-CMM010-MOIS-BIN)     CMM010AK
                           FROM WS-CMM010-JOUR-BIN                      CMM010AK
                       ADD 1 TO WS-CMM010-MOIS-BIN                      CMM010AK
                    END-IF                                              CMM010AK
               END-IF                                                   CMM010AK
             ELSE                                                       CMM010AK
               IF WS-CMM010-JOUR-BIN <= 0                               CMM010AK
                 THEN                                                   CMM010AK
                   IF WS-CMM010-MOIS-BIN = 1                            CMM010AK
                     THEN                                               CMM010AK
                       SUBTRACT 1 FROM WS-CMM010-ANNE-BIN               CMM010AK
                       MOVE 12 TO WS-CMM010-MOIS-BIN                    CMM010AK
                       ADD 31 TO WS-CMM010-JOUR-BIN                     CMM010AK
                     ELSE                                               CMM010AK
                       SUBTRACT 1 FROM WS-CMM010-MOIS-BIN               CMM010AK
                       ADD WS-CMM010-NBJJ (WS-CMM010-MOIS-BIN) TO       CMM010AK
                           WS-CMM010-JOUR-BIN                           CMM010AK
                   END-IF                                               CMM010AK
               END-IF                                                   CMM010AK
           END-IF                                                       CMM010AK
           MOVE WS-CMM010-CADR-BIN TO WS-CMM010-NUM-SEM.                CMM010AK
           IF FL-CMM010-BISS                                            CMM010AK
             THEN MOVE 28 TO WS-CMM010-NBJJ (2)                         CMM010AK
           END-IF.                                                      CMM010AK
