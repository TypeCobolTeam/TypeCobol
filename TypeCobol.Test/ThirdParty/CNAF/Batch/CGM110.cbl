      *  pav appel par a7200-iomod de tous les modules gesdro       * CGBCALAK
      *  pour displayer toute erreur db2 et autre plus tard           * CGBCALAK
      *   et ecrire le meme message dans azis erreur pour exploiter *   CGBCALAK
      *    plus aisement les erreurs                                  * CGBCALAK
      ***************************************************************** CGBCALAK
                                                                        CGBCALAK
      *                                                                 CGBCALAK
      * 1. maitrise du code 30 si possibilit                           CGBCALAK
      *                                                                 CGBCALAK
      *                                                                 CGBCALAK
           IF WS-FL-ZRTGEN-PAS-OCCUR                                    CGBCALAK
            IF WS-ZENTLG = 'DO0' OR                                     CGBCALAK
               WS-ZENTLG = 'PE0'                                        CGBCALAK
             IF WS-ZIDGDD = 'ZZZ'                                       CGBCALAK
              IF WS-ZFONCT (1:1) = 'L'                                  CGBCALAK
               MOVE ZERO TO WS-ZNOCCD                                   CGBCALAK
              END-IF                                                    CGBCALAK
             ELSE                                                       CGBCALAK
              MOVE ZERO TO WS-ZNOCCD WS-ZRTGEN WS-ZRTDET                CGBCALAK
             END-IF                                                     CGBCALAK
            ELSE                                                        CGBCALAK
             MOVE ZERO TO WS-ZNOCCD WS-ZRTGEN WS-ZRTDET                 CGBCALAK
            END-IF                                                      CGBCALAK
           END-IF.                                                      CGBCALAK
      * SPECIAL CAS DE ETST RPFRENOU                                    CGBCALAK
           IF AZIS-CHEMIN-ZAPPLT (1)= 'RPFRENOU'                        CGBCALAK
            IF WS-ZENTLG = 'LOG'                                        CGBCALAK
             IF WS-ZIDGDD = 'MPL'                                       CGBCALAK
              MOVE ZERO TO WS-ZRTGEN WS-ZRTDET                          CGBCALAK
             END-IF                                                     CGBCALAK
            END-IF                                                      CGBCALAK
           END-IF.                                                      CGBCALAK
      *                                                                 CGBCALAK
      *-------------------------------------------                      CGBCALAK
      * 1'. forage si erreur dans CGRS02P                              CGBCALAK
      *                    ou dans CGRS03P                              CGBCALAK
      *-------------------------------------------                      CGBCALAK
           IF AZIS-CHEMIN-ZAPPLT (6) = 'S02'                            CGBCALAK
           OR AZIS-CHEMIN-ZAPPLT (6) = 'S03'                            CGBCALAK
      D       Display '(CGBCALAK) - traitement particulier S02/S03'     CGBCALAK
              IF WS-ZRTGEN NOT = 0                                      CGBCALAK
      D          Display '(CGBCALAK) - Forcage  0'                     CGBCALAK
                 MOVE 0 TO WS-ZRTGEN                                    CGBCALAK
              END-IF                                                    CGBCALAK
           END-IF.                                                      CGBCALAK
      *                                                                 CGBCALAK
      *-------------------------------------------                      CGBCALAK
      * 2. message display ko ou ok selon zrtgen                        CGBCALAK
      *-------------------------------------------                      CGBCALAK
      *                                                                 CGBCALAK
      D    IF WS-ZRTGEN NOT = ZERO AND NOT = 30                         CGBCALAK
      D       DISPLAY '   CGBCALAK-DISPLAY-IGAD-APRES-KO'               CGBCALAK
      D       DISPLAY '   CHEMIN = ' AZIS-CHEMIN                        CGBCALAK
      D       DISPLAY '       '                                         CGBCALAK
      D       'ZTYPAP:' WS-ZTYPAP ';'                                   CGBCALAK
      D       'ZAPPLT:' WS-ZAPPLT ';'                                   CGBCALAK
      D       'PROCED:' LKMO-ZFONCT OF LKMO-SERVICE ';'                 CGBCALAK
      D       'ZENTLG:' WS-ZENTLG ';'                                   CGBCALAK
      D       'ZIDGDD:' WS-ZIDGDD ';'                                   CGBCALAK
      D       'ZFONCT:' WS-ZFONCT ';'                                   CGBCALAK
      D       DISPLAY '       '                                         CGBCALAK
      D       'ZNOCCD:' WS-ZNOCCD ';'                                   CGBCALAK
      D       'ZLEISP:' WS-ZLEISP ';'                                   CGBCALAK
      D       'ZSOLDE:' WS-ZSOLDE ';'                                   CGBCALAK
      D       'ZDSVCM:' WS-ZDSVCM ';'                                   CGBCALAK
      D       'ZDTDEB:' WS-ZDTDEB ';'                                   CGBCALAK
      D       'ZDTFIN:' WS-ZDTFIN ';'                                   CGBCALAK
      D       DISPLAY '       '                                         CGBCALAK
      D       'ZRTGEN:' WS-ZRTGEN ';'                                   CGBCALAK
      D       'ZRTDET:' WS-ZRTDET ';'                                   CGBCALAK
      D    ELSE                                                         CGBCALAK
      D       IF WS-DISPL = 'O'                                         CGBCALAK
      D          display '   CGBCALAK-DISPLAY-IGAD-APRES-OK'            CGBCALAK
      D          display '   CHEMIN = ' AZIS-CHEMIN                     CGBCALAK
      D          display '       '                                      CGBCALAK
      D        'ZTYPAP:' WS-ZTYPAP ';'                                  CGBCALAK
      D        'ZAPPLT:' WS-ZAPPLT ';'                                  CGBCALAK
      D        'PROCED:' LKMO-ZFONCT OF LKMO-SERVICE ';'                CGBCALAK
      D        'ZENTLG:' WS-ZENTLG ';'                                  CGBCALAK
      D        'ZIDGDD:' WS-ZIDGDD ';'                                  CGBCALAK
      D        'ZFONCT:' WS-ZFONCT ';'                                  CGBCALAK
      D          display '       '                                      CGBCALAK
      D        'ZNOCCD:' WS-ZNOCCD ';'                                  CGBCALAK
      D        'ZLEISP:' WS-ZLEISP ';'                                  CGBCALAK
      D        'ZSOLDE:' WS-ZSOLDE ';'                                  CGBCALAK
      D        'ZDSVCM:' WS-ZDSVCM ';'                                  CGBCALAK
      D        'ZDTDEB:' WS-ZDTDEB ';'                                  CGBCALAK
      D        'ZDTFIN:' WS-ZDTFIN ';'                                  CGBCALAK
      D          display '       '                                      CGBCALAK
      D        'ZRTGEN:' WS-ZRTGEN ';'                                  CGBCALAK
      D        'ZRTDET:' WS-ZRTDET ';'                                  CGBCALAK
      D     END-IF                                                      CGBCALAK
      D    END-IF                                                       CGBCALAK
      D    .                                                            CGBCALAK
      *                                                                 CGBCALAK
      *-------------------------------------------                      CGBCALAK
      * 3. affichage display erreur                                     CGBCALAK
      *    ecriture sur azis-erreur                                     CGBCALAK
      *-------------------------------------------                      CGBCALAK
      *                                                                 CGBCALAK
           IF WS-ZRTGEN NOT = ZERO                                      CGBCALAK
            MOVE SPACE TO AZIS-ERREUR                                   CGBCALAK
            IF LKMO-FL-ZTYPAP-BATCH                                     CGBCALAK
      * cette zone doit alors etre renseign par le client              CGBCALAK
      * au moyen du code chaine en sa possession                        CGBCALAK
             MOVE SPACE         TO AZIS-ERREUR-IGAD-CTYTTT              CGBCALAK
            ELSE                                                        CGBCALAK
             MOVE 'TP'          TO AZIS-ERREUR-IGAD-CTYTTT              CGBCALAK
            END-IF                                                      CGBCALAK
            MOVE LKMO-NDOMAT    TO AZIS-ERREUR-IGAD-NDOMAT              CGBCALAK
            MOVE 'I'            TO AZIS-ERREUR-IGAD-TYPPBM              CGBCALAK
            MOVE 'R'            TO AZIS-ERREUR-IGAD-TYPDES              CGBCALAK
            MOVE WS-ZAPPLT      TO AZIS-ERREUR-IGAD-ZAPPLT              CGBCALAK
            MOVE LKMO-ZFONCT OF LKMO-SERVICE                            CGBCALAK
                                TO AZIS-ERREUR-IGAD-COPROC              CGBCALAK
            MOVE WS-ZENTLG      TO AZIS-ERREUR-IGAD-ZENTLG              CGBCALAK
            MOVE WS-ZIDGDD      TO AZIS-ERREUR-IGAD-ZIDGDD              CGBCALAK
            MOVE WS-ZFONCT      TO AZIS-ERREUR-IGAD-ZFONCT              CGBCALAK
            MOVE WS-ZNOCCD      TO AZIS-ERREUR-IGAD-ZNOCCD              CGBCALAK
            MOVE WS-ZTYPAP      TO AZIS-ERREUR-IGAD-ZTYPAP              CGBCALAK
            MOVE WS-ZDSVCM      TO AZIS-ERREUR-IGAD-ZDSVCM              CGBCALAK
            MOVE WS-ZDTDEB      TO AZIS-ERREUR-IGAD-ZDTDEB              CGBCALAK
            MOVE WS-ZDTFIN      TO AZIS-ERREUR-IGAD-ZDTFIN              CGBCALAK
            MOVE WS-ZRTGEN      TO AZIS-ERREUR-IGAD-ZRTGEN              CGBCALAK
            MOVE WS-ZRTDET      TO AZIS-ERREUR-IGAD-ZRTDET              CGBCALAK
      D     DISPLAY AZIS-ERREUR                                         CGBCALAK
           END-IF.                                                      CGBCALAK
      *                                                                 CGBCALAK
       CGBCALAK-DISPLAY-APPL-APRES.                                     CGBCALAK
      *-------------------------------------------------                CGBCALAK
      *     CGBCALAK-DISPLAY-APPL-APRES                -                CGBCALAK
      *-------------------------------------------------                CGBCALAK
            MOVE SPACE TO AZIS-ERREUR                                   CGBCALAK
            IF LKMO-FL-ZTYPAP-BATCH                                     CGBCALAK
      * cette zone doit alors etre renseign par le client              CGBCALAK
      * au moyen du code chaine en sa possession                        CGBCALAK
             MOVE SPACE         TO AZIS-ERREUR-APPL-CTYTTT              CGBCALAK
            ELSE                                                        CGBCALAK
             MOVE 'TP'          TO AZIS-ERREUR-APPL-CTYTTT              CGBCALAK
            END-IF                                                      CGBCALAK
            MOVE LKMO-NDOMAT    TO AZIS-ERREUR-APPL-NDOMAT              CGBCALAK
            MOVE 'A'            TO AZIS-ERREUR-APPL-TYPPBM              CGBCALAK
            MOVE 'R'            TO AZIS-ERREUR-APPL-TYPDES              CGBCALAK
            MOVE WS-ZAPPLT      TO AZIS-ERREUR-APPL-ZAPPLT              CGBCALAK
            MOVE LKMO-ZFONCT OF LKMO-SERVICE                            CGBCALAK
                                TO AZIS-ERREUR-APPL-COPROC              CGBCALAK
            MOVE WS-ZTYPAP      TO AZIS-ERREUR-APPL-ZTYPAP              CGBCALAK
            MOVE LKMO-CABEND    TO AZIS-ERREUR-APPL-CABEND              CGBCALAK
            MOVE LKMO-TABEND    TO AZIS-ERREUR-APPL-TABEND              CGBCALAK
            MOVE LKMO-ESRV-CRTSRV                                       CGBCALAK
                                TO AZIS-ERREUR-APPL-CRTSRV.             CGBCALAK
      D     display '   CGBCALAK-DISPLAY-APPL-APRES-OK'                 CGBCALAK
      D     DISPLAY AZIS-CHEMIN.                                        CGBCALAK
      D     DISPLAY AZIS-ERREUR.                                        CGBCALAK
      *                                                                 CGBCALAK
       XXXH-CAL-CTRL-0.                                                 CGBCALAK
      *-------------------------------------------------                CGBCALAK
      *  init zone echanges avec module accs                           CGBCALAK
      *  divers controles de forme                                      CGBCALAK
      *   selon le code sens controle qualit zampd1 zampd2             CGBCALAK
      *  appel xxxh-hhhh-ctrl-1                                         CGBCALAK
      *-------------------------------------------------                CGBCALAK
                                                                        CGBCALAK
      *    DISPLAY ' XXXH-CAL-CTRL-0 '.                                 CGBCALAK
      *1                                                                CGBCALAK
           MOVE ZERO TO WSBA-ZRESUL                                     CGBCALAK
                        LSBA-ZNOCCD2.                                   CGBCALAK
                                                                        CGBCALAK
      *2                                                                CGBCALAK
      *--  IF LSBA-ZOSENS = '=='                                        CGBCALAK
      *--  OR LSBA-ZOSENS = '<='                                        CGBCALAK
      *--  OR LSBA-ZOSENS = '<<'                                        CGBCALAK
      *--  OR LSBA-ZOSENS = '>='                                        CGBCALAK
      *--  OR LSBA-ZOSENS = '>>'                                        CGBCALAK
           IF LSBA-FL-ZOSENS-EGAEGA                                     CGBCALAK
           OR LSBA-FL-ZOSENS-INFEGA                                     CGBCALAK
           OR LSBA-FL-ZOSENS-INFINF                                     CGBCALAK
           OR LSBA-FL-ZOSENS-SUPEGA                                     CGBCALAK
           OR LSBA-FL-ZOSENS-SUPSUP                                     CGBCALAK
             MOVE LSBA-ZAMPD1 TO LSBA-ZAMPD2                            CGBCALAK
                                 WSBA-ZAMPD0                            CGBCALAK
             PERFORM ABANA-CAL-CTRL-1                                   CGBCALAK
           ELSE                                                         CGBCALAK
      *--    IF LSBA-ZOSENS = '><'                                      CGBCALAK
             IF LSBA-FL-ZOSENS-INFSUP                                   CGBCALAK
             OR LSBA-FL-ZOSENS-AVTAPS                                   CGBCALAK
               MOVE LSBA-ZAMPD1 TO WSBA-ZAMPD0                          CGBCALAK
               PERFORM ABANA-CAL-CTRL-1                                 CGBCALAK
               IF WSBA-ZRESUL = ZERO                                    CGBCALAK
                 MOVE LSBA-ZAMPD2 TO WSBA-ZAMPD0                        CGBCALAK
                 PERFORM ABANA-CAL-CTRL-1                               CGBCALAK
                 IF WSBA-ZRESUL = ZERO                                  CGBCALAK
                   IF LSBA-ZAMPD1 > LSBA-ZAMPD2                         CGBCALAK
      D              DISPLAY '(BCAL) ERREUR XXXH-CAL-CTRL-0 A'          CGBCALAK
                     MOVE 8 TO WSBA-ZRESUL                              CGBCALAK
                   END-IF                                               CGBCALAK
                 END-IF                                                 CGBCALAK
               END-IF                                                   CGBCALAK
             ELSE                                                       CGBCALAK
      D        DISPLAY '(BCAL)ERREUR XXXH-CAL-CTRL-0 B'                 CGBCALAK
               MOVE 8 TO WSBA-ZRESUL                                    CGBCALAK
             END-IF                                                     CGBCALAK
           END-IF.                                                      CGBCALAK
      D    IF WSBA-ZRESUL not = zero                                    CGBCALAK
      D      display '(BCAL) -CTRL-0 WSBA-ZRESUL : ' WSBA-ZRESUL        CGBCALAK
      D    end-if.                                                      CGBCALAK
